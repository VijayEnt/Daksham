<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>codereg</servlet-name>
        <servlet-class>com.daksham.servlet.codereg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>groupreg</servlet-name>
        <servlet-class>com.daksham.servlet.groupreg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>state</servlet-name>
        <servlet-class>com.daksham.servlet.statereg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cityreg</servlet-name>
        <servlet-class>com.daksham.servlet.cityreg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.daksham.servlet.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>party</servlet-name>
        <servlet-class>com.daksham.servlet.party</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>transport</servlet-name>
        <servlet-class>com.daksham.servlet.transport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>item</servlet-name>
        <servlet-class>com.daksham.servlet.item</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.daksham.servlet.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>codereg</servlet-name>
        <url-pattern>/jsp/codereg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>groupreg</servlet-name>
        <url-pattern>/jsp/groupreg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>state</servlet-name>
        <url-pattern>/jsp/statereg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cityreg</servlet-name>
        <url-pattern>/jsp/cityreg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/jsp/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>party</servlet-name>
        <url-pattern>/jsp/party</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>transport</servlet-name>
        <url-pattern>/jsp/transport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>item</servlet-name>
        <url-pattern>/jsp/item</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
